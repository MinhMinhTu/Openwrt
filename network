config interface eth0 
	option ifname eth0
	option proto dhcp
	option hostname wifiex4u
config interface 'eth1'
	option ifname 'eth1'
	option proto 'static'
	option ipaddr '172.16.1.1'
	option netmask '255.255.255.0'
	option dns 8.8.8.8 4.2.2.2


